{
  "name": "agentdb",
  "version": "1.0.12",
  "description": "Ultra-fast agent memory and vector database with ReasoningBank for AI agents. Built on SQLite with QUIC sync. From ruv.io - Advanced AI Infrastructure.",
  "keywords": [
    "agent",
    "agentdb",
    "agent-memory",
    "vector",
    "database",
    "sqlite",
    "embeddings",
    "ai",
    "wasm",
    "quic",
    "similarity-search",
    "machine-learning",
    "vector-database",
    "semantic-search",
    "agentic",
    "claude",
    "mcp",
    "reasoning",
    "memory",
    "rag",
    "ruv",
    "ruvnet"
  ],
  "author": "rUv (@ruvnet) <team@agentic-flow.dev>",
  "license": "MIT OR Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ruvnet/agentic-flow.git",
    "directory": "packages/agentdb"
  },
  "bugs": {
    "url": "https://github.com/ruvnet/agentic-flow/issues"
  },
  "homepage": "https://agentdb.ruv.io",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "agentdb": "./bin/agentdb.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./reasoningbank/adapter/agentdb-adapter": {
      "types": "./dist/reasoningbank/adapter/agentdb-adapter.d.ts",
      "import": "./dist/reasoningbank/adapter/agentdb-adapter.js",
      "require": "./dist/reasoningbank/adapter/agentdb-adapter.js"
    },
    "./reasoningbank/adapter/types": {
      "types": "./dist/reasoningbank/adapter/types.d.ts",
      "import": "./dist/reasoningbank/adapter/types.js",
      "require": "./dist/reasoningbank/adapter/types.js"
    },
    "./reasoningbank/migration/migrate": {
      "types": "./dist/reasoningbank/migration/migrate.d.ts",
      "import": "./dist/reasoningbank/migration/migrate.js",
      "require": "./dist/reasoningbank/migration/migrate.js"
    },
    "./reasoningbank/cli/commands": {
      "types": "./dist/reasoningbank/cli/commands.d.ts",
      "import": "./dist/reasoningbank/cli/commands.js",
      "require": "./dist/reasoningbank/cli/commands.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "bin",
    "examples",
    "README.md",
    "CHANGELOG.md",
    "LICENSE-MIT",
    "LICENSE-APACHE"
  ],
  "scripts": {
    "prepack": "npm run build",
    "build": "npm run build:ts && npm run build:wasm && npm run build:browser",
    "build:ts": "tsc && tsc -p tsconfig.esm.json && node scripts/build-esm.js",
    "build:native": "echo 'Native backend uses better-sqlite3 (already built)'",
    "build:wasm": "node scripts/copy-wasm.js",
    "build:browser": "node scripts/build-browser.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:wasm": "jest --testPathPattern=wasm-backend",
    "test:native": "jest --testPathPattern=native-backend",
    "test:backends": "jest --testPathPattern=backend",
    "bench": "ts-node benchmarks/sync-performance.bench.ts",
    "bench:comprehensive": "ts-node benchmarks/comprehensive-performance.bench.ts",
    "bench:backends": "jest --testPathPattern=backend-comparison --verbose",
    "lint": "eslint src tests --ext .ts",
    "format": "prettier --write 'src/**/*.ts'",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "verify": "node scripts/verify-install.js",
    "mcp": "node bin/agentdb.js mcp"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.1",
    "ajv": "^8.12.0",
    "better-sqlite3": "^9.2.2",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "msgpackr": "^1.10.1",
    "sql.js": "^1.13.0",
    "yaml": "^2.3.4",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/inquirer": "^9.0.9",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/sql.js": "^1.4.9",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "benchmark": "^2.1.4",
    "esbuild": "^0.25.11",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
